# Labellerr API Configuration
# Get these credentials from Labellerr support (support@labellerr.com)

# Your Labellerr API key
LABELLERR_API_KEY=your_api_key_here

# Your Labellerr API secret
LABELLERR_API_SECRET=your_api_secret_here

# Your Labellerr client ID
LABELLERR_CLIENT_ID=your_client_id_here

# Optional: Set log level (debug, info, warn, error)
LOG_LEVEL=info

# Optional: API timeout in milliseconds
API_TIMEOUT=30000
